
(def empty (vector))
(def vick (vector 8 6 7 5 3 0 9))

(test 'vector-access (and? (= vick.0 8) (= vick.3 5) (= vick.6 9)))
(test 'vector-apply (= (vick 5) 0)) 

(test 'vector-type        (= vick.type 'vector))
(test 'vector-not-null    (= empty.null? false))
(test 'vector-view        (= vick.view '#(8 6 7 5 3 0 9)))
(test 'vector-false       (= empty.to-bool false))
(test 'vector-true        (= vick.to-bool true))
(test 'vector-to-list     (= vick.to-list '(8 6 7 5 3 0 9)))
(test 'vector-len         (= vick.len 7))
(test 'vector-has?        (= (vick.has? 7) true))
(test 'vector-has?-not    (= (vick.has? 4) false))

(test 'vector-fold
    (= (vick.fold
            0
            (fn (i acc x) (+ acc x))) 38))

(test 'vector-foldr
    (= (vick.foldr
            0
            (fn (i acc x) (+ acc x))) 38))

(test 'vector-map
    (= (vick.map
            (fn (i x) (* 10 x)))
            '#(80 60 70 50 30 0 90)))

(test 'vector-filter  (= (vick.filter (fn (x) (> x 5))) '#(8 6 7 9)))
(test 'vector-sort-<    (= (vick.sort <) '#(0 3 5 6 7 8 9)))
(test 'vector-sort->    (= (vick.sort >) '#(9 8 7 6 5 3 0)))


