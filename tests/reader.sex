
(sys.test 'quote
    (and
        (= 'foo (quote foo))
        (= '(1 2) (quote (1 2)))))

(sys.test 'quasiquote
    (let (foo 1 bar '(1 2) baz '(3 4 5))
        (= (list 'foo bar 3 4 5)
          `(foo ,bar @baz))))

(sys.test 'rune-literal
    (= $f (send "foo" 0)))

(sys.test 'pair-literal
    (= (pair 'x 'y)
       '(pair: x y)))

(sys.test 'vector-literal
    (= (vector 1 2 (list 3 4 5))
       (vector: 1 2 (3 4 5))))

(sys.test 'matrix-literal
    (= (matrix (1 2) (3 4))
       (# (1 2) (3 4))))

(sys.test 'record-literal
    (= (record 'x 1 'y (list 'a 2))
       (record: x 1 y (a 2))))

