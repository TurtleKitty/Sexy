
(lib () pass)

; (seq expr) -> expr

(proc pass (form)
    (if (pair? form)
        (cond
            (= form.head 'quote)
                form
            (and (= form.head 'seq) (= form.size 2))
                (pass form.1)
            default:
                (pair (pass form.head) (pass form.tail)))
        form))

