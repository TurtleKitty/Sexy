
(use gen-pass "generic.sxy")

(lib () pass)

; (seq expr) -> expr

(proc pass (form)
    (if (pair? form)
        (if (= 'seq form.head)
            (if (= form.size 2)
                (pass form.1)
                %(seq @(pass form.tail)))
            ((gen-pass 'application) pass form))
        form))

