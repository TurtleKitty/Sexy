
(use gen-pass "generic.sxy")

(lib () pass)

; (seq (seq x y) z) -> (seq x y z)

(def pass
    (gen-pass (:
        'seq (proc (form)
                (if (and (pair? form.1) (= form.1.head 'seq))
                    (pass %(seq @form.1.tail @form.tail.tail))
                    %(seq @(pass form.tail)))))))

