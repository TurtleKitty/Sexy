
(use gen-pass "generic.sxy")

(lib () pass)

; (seq 1 \F foo "foo" (vector: 1 2 3))
; -> (seq (quote 1) (quote \F) foo (quote "foo") (quote (vector: 1 2 3)))

(proc pass (form)
    (if (pair? form)
        (if (option? form.head)
            %(quote $form)
            ((gen-pass 'application) pass form))
        (if (or (symbol? form) (= form ()))
            form
            %(quote $form))))

